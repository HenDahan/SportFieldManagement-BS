<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReservationActivity">

    <Button
        android:id="@+id/goBackBtn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RemoveBtn" />

    <TextView
        android:id="@+id/HeadLine"
        style="@android:style/Widget.DeviceDefault.TextView"
        android:layout_width="162dp"
        android:layout_height="37dp"
        android:text="הפעילויות שלי"
        android:textSize="24sp"
        tools:ignore="MissingConstraints,UnknownId"
        tools:layout_editor_absoluteX="154dp"
        tools:layout_editor_absoluteY="70dp" />

    <ListView
        android:id="@+id/listActivity"
        android:layout_width="420dp"
        android:layout_height="397dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="177dp" />

    <Button
        android:id="@+id/RemoveBtn"
        android:layout_width="136dp"
        android:layout_height="39dp"
        android:text="הסר פעילות"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="604dp" />
</androidx.constraintlayout.widget.ConstraintLayout>